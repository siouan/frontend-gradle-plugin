    "eslint": "~7",
    "eslint-config-prettier": "~8",
    "eslint-plugin-nuxt": "~3",
    "eslint-plugin-prettier": "~4",
    "fibers": "~5",
    "ncp": "~2",
    "node-gyp": "~9",
    "prettier": "~2",
    "rimraf": "~5",
    "sass": "~1",
    "sass-loader": "~10"
